function create() {
    cd
    python3 create.py $1
    mkdir /Users/dnikhilesh/Documents/Projects
    cd /Users/Dnikhilesh/Documents/Projects
    touch README.md
    git init
    git remote add origin https://github.com/ferran9908/$1.git
    git add -A
    git commit -m "Initial Commit"
    git push origin master
    code .
}
